{
    "name": "codekanzlei/cake-model-history",
    "description": "ModelHistory plugin for CakePHP3",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "ModelHistory"
    },
    "require": {
        "php": ">=5.4",
        "codekanzlei/cake-cktools": "^1.3",
        "codekanzlei/cake-frontend-bridge": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "cakephp/cakephp": "3.3.*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ModelHistory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModelHistory\\": "src",
            "ModelHistory\\Test\\": "tests",
            "ModelHistory\\Test\\Fixture\\": "tests/Fixture",
            "ModelHistoryTestApp\\": "tests/ModelHistoryTestApp"
        }
    }
}
